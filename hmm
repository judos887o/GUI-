    local SCALE = 1
    if Shared.Platform == "Mobile" or UserInputService.TouchEnabled then
        SCALE = 1 / 1.5
    end

    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "SchmecktUI"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.Parent = CoreGui
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    local Theme = {
        Background = Color3.fromRGB(15, 5, 5),
        Card = Color3.fromRGB(45, 10, 10),
        Accent = Color3.fromRGB(220, 0, 0),
        Text = Color3.fromRGB(255, 255, 255),
        SubText = Color3.fromRGB(255, 150, 150),
        Stroke = Color3.fromRGB(80, 0, 0),
        Shadow = Color3.fromRGB(0, 0, 0)
    }

    local function scaleUDim(x, y) return UDim2.new(0, x * SCALE, 0, y * SCALE) end
    local function scaleSize(s) return math.floor(s * SCALE) end

    -- BOTTOM BAR & SHADOW
    local BottomBarShadow = Instance.new("Frame")
    BottomBarShadow.Name = "BottomBarShadow"
    BottomBarShadow.Size = scaleUDim(510, 65)
    BottomBarShadow.Position = UDim2.new(0.5, -(255 * SCALE), 1, -(115 * SCALE))
    BottomBarShadow.BackgroundColor3 = Theme.Shadow
    BottomBarShadow.BackgroundTransparency = 0.7
    BottomBarShadow.BorderSizePixel = 0
    BottomBarShadow.Parent = ScreenGui
    Instance.new("UICorner", BottomBarShadow).CornerRadius = UDim.new(0, 14 * SCALE)

    local BottomBar = Instance.new("Frame")
