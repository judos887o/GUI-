    SubTitle.TextColor3 = Theme.SubText
    SubTitle.TextXAlignment = Enum.TextXAlignment.Left
    SubTitle.Parent = BottomBar

    local StatsFrame = Instance.new("Frame")
    StatsFrame.Size = UDim2.new(0, 120 * SCALE, 0, 40 * SCALE)
    StatsFrame.Position = UDim2.new(1, -(135 * SCALE), 0.5, -(20 * SCALE))
    StatsFrame.BackgroundColor3 = Theme.Card
    StatsFrame.BorderSizePixel = 0
    StatsFrame.Parent = BottomBar
    Instance.new("UICorner", StatsFrame).CornerRadius = UDim.new(0, 8 * SCALE)

    local StatsLabel = Instance.new("TextLabel")
    StatsLabel.Name = "Stats"
    StatsLabel.Size = UDim2.new(1, -(16 * SCALE), 1, 0)
    StatsLabel.Position = UDim2.new(0, 8 * SCALE, 0, 0)
    StatsLabel.BackgroundTransparency = 1
    StatsLabel.Text = "FPS: --\nPing: --"
    StatsLabel.Font = Enum.Font.GothamBold
    StatsLabel.TextSize = scaleSize(10)
    StatsLabel.TextColor3 = Theme.Text
    StatsLabel.TextXAlignment = Enum.TextXAlignment.Left
    StatsLabel.Parent = StatsFrame

    task.spawn(function()
        while BottomBar.Parent do
            local fps = math.floor(1 / (RunService.RenderStepped:Wait() + 0.001))
            local ping = 0
            pcall(function() ping = math.floor(Stats.Network.ServerStatsItem["Data Ping"]:GetValue()) end)
            StatsLabel.Text = "FPS: " .. fps .. "\nPing: " .. ping .. "ms"
            task.wait(0.5)
        end
    end)

