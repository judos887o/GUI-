    local UICorner = Instance.new("UICorner"); UICorner.CornerRadius = UDim.new(0, 12 * SCALE); UICorner.Parent = BottomBar
    local UIStroke = Instance.new("UIStroke"); UIStroke.Color = Theme.Stroke; UIStroke.Thickness = 1.5 * SCALE; UIStroke.Transparency = 0; UIStroke.Parent = BottomBar

