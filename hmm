    BottomBar.Size = scaleUDim(500, 55)
    BottomBar.Position = UDim2.new(0.5, -(250 * SCALE), 1, -(120 * SCALE))
    BottomBar.BackgroundColor3 = Theme.Background
    BottomBar.BorderSizePixel = 0
